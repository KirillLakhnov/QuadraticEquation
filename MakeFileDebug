all: 
	g++ maintests.cpp common.cpp QuadraticEquation.cpp tests.cpp -o quadraticTest

quadraticTest: maintests.o common.o QuadraticEquation.o tests.o
	g++ maintests.o common.o QuadraticEquation.o tests.o 

maintests.o: maintests.cpp
	g++ c maintests.cpp

common.o: common.cpp
	g++ c common.cpp

QuadraticEquation.o: QuadraticEquation.cpp
	g++ c QuadraticEquation.cpp

tests.o: tests.cpp
	g++ c tests.cpp

clean:
	rm -rf *.o quadratic
